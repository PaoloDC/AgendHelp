<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.paolo.agendhelp">

    <application
        android:allowBackup="true"
        android:icon="@drawable/imgapp"
        android:label="@string/app_name"
        android:roundIcon="@drawable/imgapp"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".InserisciActivity"/>
        <activity android:name=".Allarme" android:noHistory="true"/>
        <activity android:name=".LoginActivity"/>
        <activity android:name=".RegistrazioneActivity"/>
    </application>

</manifest>